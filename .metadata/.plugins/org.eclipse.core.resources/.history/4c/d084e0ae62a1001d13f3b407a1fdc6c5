package com.nv.foodapp.repository.item;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

import com.nv.foodapp.entity.Item;

@Repository
public interface ItemRepository extends JpaRepository<Item,Integer>{
   
	public Item addItem(Item item);
	public Item viewItemById(int id);
	public Item updateRestaurantBYItem(int restaurantid,int itemid);
	public List<Item> viewAllItems();
	public Item viewAllItemsByName(String name);
	
   	
}
